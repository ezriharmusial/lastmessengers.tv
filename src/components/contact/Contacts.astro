---
import { Image } from "@astrojs/image/components";
import { Avatar } from '@skeletonlabs/skeleton';
import Icon from 'astro-icon'
export let sectionClass = "w-1/2";
---

<div class="container bg-dark text-light pt-18">
	<div class="grid">

		<div class="card overflow-hidden shadow-xl max-w-xs my-3 variant-glass-primary">
			<image loading="lazy" src="/images/contacts/Ezehk-2.jpg"
			alt="Ezekwesiri's Profile Picture"
			width={640}
			aspectRatio={16/9} class="w-full"/>
			<div class="flex justify-center -mt-8">
				<Avatar width="w-24" src="/images/contacts/Ezehk-1-7090-150x150.jpg" alt="Ezekwesiri's Wallpaper Picture" class="-mt-3" />
				<!-- <img src="https://i.imgur.com/8Km9tLL.jpg" class="rounded-full border-solid border-white border-2 -mt-3"> -->
			</div>
			<div class="text-center px-3 pb-6 pt-2">
				<h3 class="text-white text-sm bold font-sans">Eng. Ezekwesiri E. Obioha</h3>
				<p class="text-white text-sm bold font-sans">CEO: ArtKids Foundation</p>
				<p class="mt-2 font-sans font-light text-white">
					e: <a href="mailto:info@artkidsfoundation.org"
					class="has-text-secondary"
					title="Email Eng. Ezekwesiri E. Obioha">info@artkidsfoundation.org</a>
					<br>
					t: <a href="tel:+234 (0)81 601 451 77"
					class="has-text-secondary"
					title="Call Eng. Ezekwesiri E. Obioha ">+234 (0)81 601 451 77
				</a>
			</p>
		</div>
		<div class="flex justify-center gap=4 pb-3 text 2xl">
				<a class="btn variant-ringed-secondary h-4 w-4 m-2" target="_blank" href="https://www.facebook.com/profile.php?id=100015798102483">
					<Icon pack="fa" name="facebook" />
				</a>
				<a class="btn variant-ringed-secondary h-4 w-4 m-2" target="_blank" href="https://www.linkedin.com/pub/engr-obioha-ezekwesiri-ict/2b/ab6/421">
					<Icon pack="fa" name="linkedin" />
				</a>
				<a class="btn variant-ringed-secondary h-4 w-4 m-2" target="_blank" href="https://twitter.com/ObiohaEzekwesir">
					<Icon pack="fa" name="twitter" />
				</a>
		</div>
	</div>

	<section class={sectionClass}>
		<div class="card card-hover p-4">
			<header class="card-header ">
				<image loading="lazy" src="/images/contacts/Ezehk-2.jpg"
				alt="Ezekwesiri's Profile Picture"
				width={640}
				aspectRatio={16/9}/>
			</header>
			<div class="">
				<Avatar width="w-56" src="/images/contacts/Ezehk-1-7090-150x150.jpg" alt="Ezekwesiri's Wallpaper Picture" />
				<p class="title is-size-5 has-text-secondary">Eng. Ezekwesiri E. Obioha</p>
				<p class="subtitle is-size-6 has-text-light">CEO: ArtKids Foundation</p>
				<p>
					e: <a href="mailto:info@artkidsfoundation.org"
					class="has-text-secondary"
					title="Email Eng. Ezekwesiri E. Obioha">info@artkidsfoundation.org</a>
					<br>
					t: <a href="tel:+234 (0)81 601 451 77"
					class="has-text-secondary"
					title="Call Eng. Ezekwesiri E. Obioha ">+234 (0)81 601 451 77
				</a>
			</p>
			<footer class="card-footer">
				<a class="btn variant-ringed-secondary h-4 w-4" target="_blank" href="https://www.facebook.com/profile.php?id=100015798102483">
					<Icon pack="fa" name="facebook" />
				</a>
				<a class="btn variant-ringed-secondary h-4 w-4" target="_blank" href="https://www.linkedin.com/pub/engr-obioha-ezekwesiri-ict/2b/ab6/421">
					<Icon pack="fa" name="linkedin" />
				</a>
				<a class="btn variant-ringed-secondary h-4 w-4" target="_blank" href="https://twitter.com/ObiohaEzekwesir">
					<Icon pack="fa" name="twitter" />
				</a>
			</footer>

		</section>

		<section class={sectionClass}>
			<h4 class="title">Managing Director: ArtKids Foundation Europe</h4>
			<div class="card is-profile has-background-dark">
				<div class="card-image">
					<figure>
						<div class="dcll-bgwrap" style="background-color:#812d06;"></div>
						<Image
						src="/images/contacts/Biblequotes-Facebook-Shared-Image-Rastered-9-343x180.jpg"
						width={640	}
						alt="Bas Bijlevelds's Wallpaper Picture"
						aspectRatio={16/9}/>
					</figure>
				</div>
				<div class="card-profile-image">
					<figure class="image is-128x128">
						<Avatar src="/images/contacts/bas_profielfoto-150x150.jpg"
						alt="Profielfoto Bas Bijleveld"/>
					</figure>
				</div>
				<div class="card-content content text-center has-text-light">
					<p class="title is-size-5 has-text-secondary">Bastiaan M. Bijleveld</p>
					<p class="subtitle is-size-6 has-text-light">Managing Director: ArtKids Foundation Europe</p>
					<p>
						e: <a href="mailto:europe@artkidsfoundation.org"
						class="has-text-secondary"
						title="Email Bas Bijleveld">europe@artkidsfoundation.org</a>
						<br>
						t: <a href="tel:+31(0)6 285 86 335"
						class="has-text-secondary"
						title="Call Bas Bijleveld">+31(0)6 285 86 335</a>
					</p>
				</div>
				<footer class="card-footer flex">
					<a class="btn card-footer-item has-text-secondary h-4 w-4" target="_blank" href="https://www.facebook.com/Basbijleveld">
						<Icon pack="fa" name="facebook" />
					</a>
					<a class="btn card-footer-item has-text-secondary h-4 w-4" target="_blank" href="https://nl.linkedin.com/in/basbijleveld">
						<Icon pack="fa" name="linkedin" />
					</a>
					<a class="btn card-footer-item has-text-secondary h-4 w-4" target="_blank" href="https://www.youtube.com/channel/UC6Kzb3a2xKltZCHdYtHsvcQ">
						<Icon pack="fa" name="youtube" />
					</a>
				</footer>
			</div>
		</section>

		<section class={sectionClass}>
			<h4 class="title">Director of International Business</h4>
			<div class="card is-profile has-background-dark has-text-light">
				<div class="card-image">
					<figure>
						<div class="dcll-bgwrap" style="background-color:#779aa8;"></div>
						<image loading="lazy" src="/images/contacts/Biblequotes-Facebook-Shared-Image-Rastered-343x180.jpg"
						alt="Ezri's Wallpaper images"
						width={640}
						aspectRatio={16/9} />
					</figure>
				</div>
				<div class="card-profile-image">
					<figure class="image is-128x128">
						<Avatar src="/images/contacts/Ezri_0.jpg"
						alt="Profielfoto Ezri" />
					</figure>
				</div>
				<div class="card-content content text-center">
					<p class="title is-size-5 has-text-secondary">Ezri D. Harmusial</p>
					<p class="subtitle has-text-light is-size-6">Director of International Business: ArtKids Foundation</p>
					<p>
						e: <a href="mailto:international@artkidsfoundation.org"
						class="has-text-secondary"
						title="Email Ezri Harmusial">international@artkidsfoundation.org</a>
						<br>
						t: <a
						class="has-text-secondary"
						href="tel:+31(0)6 285 86 335" title="Call Ezri Harmusial">+31(0)6 285 86 335</a>
					</p>
				</div>
				<footer class="card-footer flex">
					<a class="btn card-footer-item has-text-secondary h-4 w-4" target="_blank" href="https://www.facebook.com/ezriharmusial">
						<Icon pack="fa" name="facebook" />
					</a>
					<a class="btn card-footer-item has-text-secondary h-4 w-4" target="_blank" href="https://nl.linkedin.com/in/ezri-harmusial">
						<Icon pack="fa" name="linkedin" />
					</a>
					<a class="btn card-footer-item has-text-secondary h-4 w-4" target="_blank" href="https://twitter.com/ezriharmusial">
						<Icon pack="fa" name="twitter" />
					</a>
				</footer>
			</div>
		</section>

		<section class={sectionClass}>
			<h4 class="title">Director: ArtKids Foundation Africa</h4>
			<div class="card is-profile has-background-dark">
				<div class="card-image">
					<figure>
						<div class="dcll-bgwrap" style="background-color:#635c57;"></div>
						<a href="/contact">
							<image loading="lazy" src="/images/contacts/Biblequotes-Facebook-Shared-Image-Rastered-2-343x180.jpg"
							alt="Chibundu's Wallpaper"
							width={640}
							aspectRatio={16/9}/>
						</a>
					</figure>
				</div>
				<div class="card-profile-image">
					<figure class="image is-128x128">
						<Avatar src="/images/contacts/chibundu_mmeri_victory.jpg" alt="Chibundu's Profile Picture" />
					</figure>
				</div>
				<div class="card-content content text-center has-text-light">
					<p class="title is-size-5 has-text-secondary"> Chibundu Mmeri Obioha</p>
					<p class="subtitle is-size-6 has-text-light">Director: ArtKids Foundation Africa</p>
					<p>
						e: <a href="mailto:africa@artkidsfoundation.org"
						class="has-text-secondary"
						title="Email Chibundu Mmeri Obioha">africa@artkidsfoundation.org</a>
						<br>
						t: <a href="tel:+234 (0)81 484 874 27"
						class="has-text-secondary"
						title="Call Chibundu Mmeri Victory">+234 (0)81 484 874 27
					</a>
				</p>
			</div>
		</div>
	</section>
</div>
</div>