// TypeScript interface of openGraph prop
interface openGraph {
	basic: {
		title: string;
		type: string;
		image: string;
		url: string;
	},
	optional?: {
		audio?: string;
		description?: string;
		determiner?: string;
		locale?: string;
		localeAlternate?: Array<string>;
		siteName?: string;
		video?: string;
	},
	image?: {
		alt?: string;
	}
}

