{
  "backend": {
    "name": "git-gateway",
    "branch": "main"
  },
  "local_backend": true,
  "collections": [
    {
      "name": "Blog",
      "label": "Messages",
      "label_singular": "Message",
      "folder": "src/pages/blog",
      "path": "{{slug}}/index.md",
      "preview_path": "blog/{{title}}",
      "create": true,
      "fields": [
          {"label": "Title", "name": "title", "widget": "string"},
          {"label": "Publish Date", "name": "date", "widget": "datetime"},
          {"label": "Featured Image", "name": "thumbnail", "widget": "image"},
          {"label": "Body", "name": "body", "widget": "markdown"},
        {
          "name": "theme",
          "widget": "select",
          "options": [
            "dark",
            "light"
          ],
          "default": "dark",
          "label": "VFX: Dark or Light Theme",
          "required": false
        },
        {
          "name": "alignImage",
          "widget": "select",
          "options": [
            "left",
            "center",
            "right"
          ],
          "label": "VFX: Align Artist Image VS Text",
          "required": false
        },
        {
          "name": "bgColor",
          "label": "VFX: Tailwind Background Class",
          "required": false
        },
        {
          "name": "color",
          "label": "VFX: Tailwind Text Color Class",
          "required": false
        },
        {
          "name": "bg",
          "label": "VFX: Giphy Image name",
          "required": false
        },
        {
          "name": "bgBlend",
          "label": "VFX: Tailwind Background Blend Class",
          "widget": "select",
          "options": [
            "bg-blend-normal",
            "bg-blend-multiply",
            "bg-blend-screen",
            "bg-blend-overlay",
            "bg-blend-darken",
            "bg-blend-lighten",
            "bg-blend-color-dodge",
            "bg-blend-color-burn",
            "bg-blend-hard-light",
            "bg-blend-soft-light",
            "bg-blend-difference",
            "bg-blend-exclusion",
            "bg-blend-hue",
            "bg-blend-saturation",
            "bg-blend-color",
            "bg-blend-luminosity"
          ],
          "required": false
        },
        {
          "name": "bgSize",
          "widget": "select",
          "options": [
            "bg-cover",
            "bg-contain",
            "bg-auto"
          ],
          "label": "VFX: Background Size",
          "required": false
        },
        {
          "name": "bgPosition",
          "widget": "select",
          "options": [
            "bg-bottom",
            "bg-center",
            "bg-left",
            "bg-left-bottom",
            "bg-left-top",
            "bg-right",
            "bg-right-bottom",
            "bg-right-top",
            "bg-top"
          ],
          "label": "VFX: Align background image",
          "required": false
        },
        {
          "name": "description",
          "label": "SEO: Description 155 Characters",
          "required": false
        },
        {
          "name": "twitterImage",
          "label": "SEO: Image used in twitter",
          "widget": "image",
          "required": false,
          "media_folder": "public/uploads/coverimages/twitter",
          "public_folder": "/uploads/coverimages/twitter"
